struct Fibonacci has 

    prv def fibonacci_recursive_helper(self, a, b, counter, len) do
        if $counter <= $len do
            println $a;

            $self.fibonacci_recursive_helper $self, $b, ($a + $b), $counter + 1, $len; 
        end

    end

    pub def fibonacci_recursive(self, len) do
        -- start the sequence
        $self.fibonacci_recursive_helper $self, 0, 1, 1, $len;
    end
end     