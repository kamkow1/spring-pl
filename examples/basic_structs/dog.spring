struct Dog has
    pub prop age;

    pub def bark() do
        println "woof woof";
    end

    pub def run(dist) do
        loop 0 to $dist with "d" do

            -- dog teleports from 15m to 20m
            if $d >= 15 and $d <= 20 do 
                println "teleporting from 15m to 20m";
                go next;
            end

            println ("the dog ran " + string $d) + " meters";
        end
    end
end