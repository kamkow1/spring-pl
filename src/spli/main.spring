struct Dog {
    pub prop age;

    prv def bark() {
        println "woof woof";
    }

    pub def run(self, dist) {
        loop 0 to $dist with "d" {
            println ("the dog ran " + string $d) + " meters";
            $self.bark();
        }
    }
}

def main(args) {
    new Dog -> $dog;

    7 -> $dog.age;

    println $dog.age;

    $dog.run $dog, 30;

    declare 7 -> $num;

    67 -> $num;

    println $num;
}